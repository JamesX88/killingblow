{
  "rules": {
    ".read": "auth != null",
    ".write": "auth != null",
    "players": {
      ".indexOn": ["killingBlows", "bossTokens", "gold", "lifeTokens", "deathTokens"]
    },
    "pvpMatches": {
      ".indexOn": "status"
    },
    "chat": {
      ".read": true,
      ".write": "auth != null"
    },
    "boss": {
      ".read": true,
      ".write": "auth != null"
    },
    "killedBosses": {
      ".read": true,
      ".write": "auth != null"
    },
    "pvpQueue": {
      ".read": "auth != null",
      ".write": "auth != null"
    }
  }
}
